<header>
  <h1>ねこったー</h1>
</header>
<div class="contents">
  <h2>投稿する</h2>
  <%= form_tag('/tweets',method: :post ,class: "clearfix" ) do %>
    <%= image_tag 'cat1.png' %>
    <div class="input-box">
      <textarea name="text" rows="4" cols="40"></textarea>
    </div>
    <div class="button-box">
      <input type="submit" class="submit-button">
    </div>
  <% end %>
</div>

